{
  "name": "metronome",
  "version": "1.0.0",
  "type": "module",
  "main": "electron/main.mjs",
  "scripts": {
    "test": "NODE_OPTIONS='--no-deprecation' jest",
    "test:watch": "node --experimental-vm-modules node_modules/.bin/jest --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "dev": "vite",
    "dev:webpack": "webpack serve --mode development",
    "build:prod": "NODE_ENV=production webpack --mode production",
    "build": "vite build",
    "start": "npm run build:prod && electron ."
  },
  "build": {
    "appId": "com.example.metronome",
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ]
    }
  },
  "keywords": [
    "metronome",
    "music",
    "electron",
    "app"
  ],
  "author": "dolamore",
  "license": "MIT",
  "description": "A simple metronome application built with Electron.",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.26.0",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react": "^4.3.4",
    "css-loader": "^7.1.2",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "html-webpack-plugin": "^5.6.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.5.3",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite-plugin-babel": "^1.3.0",
    "vite-plugin-commonjs": "^0.10.4"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.14",
    "@nestjs/core": "^11.0.14",
    "@nestjs/platform-express": "^11.0.14",
    "axios": "^1.9.0",
    "babel-loader": "^10.0.0",
    "flatted": "^3.3.3",
    "lodash": "^4.17.21",
    "mobx": "^6.13.6",
    "mobx-react": "^9.2.0",
    "mobx-react-lite": "^4.1.0",
    "punycode2": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "tone": "^15.0.4",
    "ts-node": "^10.9.2",
    "vite": "^6.2.2",
    "vite-plugin-html": "^3.2.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "winston": "^3.17.0",
    "winston-transport": "^4.9.0"
  }
}
