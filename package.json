{
  "name": "metronome",
  "version": "1.0.0",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev:vite": "vite",
    "dev:webpack": "webpack serve --mode development",
    "build:prod": "webpack --mode production",
    "build:vite": "vite build",
    "start": "electron ."
  },
  "build": {
    "appId": "com.example.metronome",
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ]
    }
  },
  "keywords": [
    "metronome",
    "music",
    "electron",
    "app"
  ],
  "author": "dolamore",
  "license": "MIT",
  "description": "A simple metronome application built with Electron.",
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "html-webpack-plugin": "^5.6.3",
    "vite-plugin-babel": "^1.3.0",
    "vite-plugin-commonjs": "^0.10.4"
  },
  "dependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "babel-loader": "^10.0.0",
    "howler": "^2.2.4",
    "mobx": "^6.13.6",
    "mobx-react": "^9.2.0",
    "mobx-react-lite": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tone": "^15.0.4",
    "vite": "^6.2.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
